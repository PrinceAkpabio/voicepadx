urn null\r\n    //  }\r\n  };\r\n\r\n  // MAKE TEXT FEILD EDITABLE\r\n  const handleChange = (e) => {\r\n    e.preventDefault();\r\n    setCurrentText(e.target.value);\r\n  };\r\n\r\n  // Update Note\r\n  const title = currentText;\r\n  const id = Id;\r\n  console.log(currentText);\r\n  const handleSubmit = async () => {\r\n    await axios.post(`http://localhost:5000/notes/note/${id}`, {\r\n      title,\r\n      id,\r\n    });\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className=\"speechTitle\">\r\n        <h2 className=\"title\">\r\n          Click the microphone and wait half a second before speaking:{\" \"}\r\n        </h2>\r\n\r\n        <span className=\"copy-container\">\r\n          <i onClick={handleCopy} id=\"copy\" className=\"far fa-copy\">\r\n            <span className=\"tooltip\">{copy}</span>\r\n          </i>\r\n        </span>\r\n      </div>\r\n      <div className=\"speech-wrapper\">\r\n        <textarea\r\n          type=\"text\"\r\n          onChange={handleChange}\r\n          className=\"text\"\r\n          value={currentText}\r\n          rows=\"5\"\r\n        />\r\n        {/* <i\r\n          id=\"mic\"\r\n          onMouseDown={play}\r\n          onClick={HandleSpeech}\r\n          className=\"fas fa-microphone-alt\"\r\n        ></i> */}\r\n        <i\r\n          id=\"mic\"\r\n          onClick={handleSubmit}\r\n          // className=\"fas fa-microphone\"\r\n        >\r\n          SUBMIT\r\n        </i>\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default UpdateNote;\r\n","C:\\Users\\user\\Documents\\My Projects\\voicepadx\\client\\voicepad\\src\\data-requests\\user.js",[],"C:\\Users\\user\\Documents\\My Projects\\voicepadx\\client\\voicepad\\src\\data-requests\\auth.header.js",[],"C:\\Users\\user\\Documents\\My Projects\\voicepadx\\client\\voicepad\\src\\components\\NewNote.jsx",["165","166","167","168","169"],"C:\\Users\\user\\Documents\\My Projects\\voicepadx\\client\\voicepad\\src\\components\\navigation\\naviagtion.j